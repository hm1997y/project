/* components/nineImg/cmp.wxss */
.container{
    width:100%;
    
}
.imgContainer{
    position: relative;
    width:700rpx;
    height:700rpx;
    display:flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: 0 auto;
}
.image{
    width:31%;
    height:31%;
}
.num{
    position: absolute;
    right:80rpx;
    bottom:80rpx;
    background-color:rgba(0, 0, 0, .7);
    color:#fff;
    padding:5rpx 10rpx;
    font-size:24rpx;
}
.textContainer{
    padding:10rpx 30rpx;
    font-size: 40rpx;
    font-weight: 400;
    letter-spacing: 2rpx;
}